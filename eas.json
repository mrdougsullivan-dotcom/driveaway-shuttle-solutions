{
  "cli": {
    "version": ">= 14.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
     "production": {
      "env": {
        "EXPO_PUBLIC_VIBECODE_BACKEND_URL": "https://driveaway-backend-production.up.railway.app"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "mr.doug.sullivan@gmail.com",
        "ascAppId": "6755795297",
        "appleTeamId": "M4NCBU3DYH"
      }
    }
  }
}
