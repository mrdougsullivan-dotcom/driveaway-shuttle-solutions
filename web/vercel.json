{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "https://preview-zgdvxfmlompr.share.sandbox.dev"
  }
}
